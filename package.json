{
  "name": "bfjs",
  "version": "0.0.0-alpha",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "start": "npm-run-all --parallel watch:server watch:build",
    "watch:server": "nodemon \"./build/bundle.js\" --watch \"./build\" ",
    "build:watch": "tsc --watch",
    "build": "rm -rf dist && tsc",
    "build:protos": "./scripts/build_protos.sh $(pwd)"
  },
  "dependencies": {
    "@types/superagent": "^3.8.4",
    "add": "^2.0.6",
    "axios": "^0.18.0",
    "big.js": "^5.2.2",
    "bignumber.js": "^7.0.1",
    "cbor": "^4.0.0",
    "chalk": "^2.4.1",
    "command-line-args": "^5.0.2",
    "eth-crypto": "^1.2.4",
    "ethereumjs-util": "^6.0.0",
    "figlet": "^1.2.1",
    "google-closure-library": "^20180405.0.0",
    "google-protobuf": "^3.5.0",
    "moment": "^2.22.2",
    "node-emoji": "^1.8.1",
    "nodemon": "^1.18.6",
    "ora": "^3.0.0",
    "prettyjson": "^1.2.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "request-promise-native": "^1.0.5",
    "sawtooth-sdk": "^1.0.2",
    "superagent": "^4.0.0",
    "ts-protoc-gen": "^0.7.7",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "vorpal": "^1.12.0",
    "web3": "^0.20.6"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.10.3",
    "ts-loader": "^5.3.0",
    "tslint-config-airbnb": "^5.11.0",
    "webpack": "^4.26.0"
  }
}